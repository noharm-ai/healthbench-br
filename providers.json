{
  "providers": [
    {
      "name": "GPT-4o",
      "type": "openai",
      "model": "gpt-4o",
      "api_key": "${OPENAI_API_KEY}",
      "base_url": "https://api.openai.com/v1",
      "temperature": 0.0,
      "max_tokens": 12000
    },
    {
      "name": "Llama3.2-Local",
      "type": "ollama",
      "model": "llama3.2",
      "base_url": "http://localhost:11434",
      "temperature": 0.0,
      "max_tokens": 12000
    },
    {
      "name": "Claude-Haiku-Bedrock",
      "type": "aws_bedrock",
      "model": "anthropic.claude-3-haiku-20240307-v1:0",
      "region": "us-east-1",
      "aws_bearer_token": "${AWS_BEARER_TOKEN_BEDROCK}",
      "temperature": 0.0,
      "max_tokens": 12000
    },
    {
      "name": "Maritaca-Large",
      "type": "maritaca",
      "model": "sabia-3-large",
      "api_key": "${MARITACA_API_KEY}",
      "base_url": "https://chat.maritaca.ai",
      "temperature": 0.0,
      "max_tokens": 12000
    }
  ],
  "default_settings": {
    "temperature": 0.0,
    "max_tokens": 12000,
    "timeout": 120,
    "parallelism": 10
  }
}